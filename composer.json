{
    "name":"netzmacht/workflow",
    "description":"Workflow library",
    "keywords":["workflow", "transition", "states"],
    "type":"library",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"David Molineus",
            "email":"mail@netzmacht.de",
            "homepage":"http://www.netzmacht.de",
            "role":"Project leader"
        }
    ],
    "support":{
        "email":"mail@netzmacht.de",
        "issues":"https://github.com/netzmacht/workflow/issues",
        "source":"https://github.com/netzmacht/workflow"
    },
    "require":{
        "php":"~5.3",
        "symfony/event-dispatcher": "~2.3",
        "beberlei/assert": "~2.0"
    },
    "require-dev": {
        "phpspec/phpspec": "2.1.*",
        "netzmacht/contao-build-tools": "dev-master",
        "henrikbjorn/phpspec-code-coverage" : "~0.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "Netzmacht\\Workflow": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra":{
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
